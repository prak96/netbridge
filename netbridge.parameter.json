{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "projectName": {
            "value": "netbridge",
            "metadata": {
                "description": "Enter the Project Name"
            }
        },
        "count": {
            "value": 2,
            "metadata": {
                "description": "Enter the number of VMs in each subnet"
            }
        },
        "storageName": {
            "value": "storage15",
            "metadata": {
                "description": "Enter the Azure Storage Name"
            }

        },
        "skuTypes": {
            "value": "Standard_LRS",
            "metadata": {
                "description": "Enter the sku of Azure Storage"
            }
        },

        "vmUsername": {
            "value": "pratul",
            "metadata": {
                "description": "Enter the username of VM"
            } 
        },
        "vmPassword": {
            "value": "VMpassword@1234",
            "metadata": {
                "description": "Enter the Password of VM"
            }
        },
        "vmOffer": {
            "value": "WindowsServer",
            "metadata": {
                "description": "Enter the OS of VM from the list"
            }
        },
        "vmPublisher": {
            "value": "MicrosoftWindowsServer",
            "metadata": {
                "description": "Enter the Publisher of VM from the list"
            }
        },
        "vmSize": {
            "value": "Standard_DS2_v2",
            "metadata": {
                "description": "Enter the size of VM"
            }
        },
        "vmSku": {
            "value": "2022-datacenter-azure-edition",
            "metadata": {
                "description": "Enter the sku of VM from the list"
            }
        },
        "vpnVNETaddressPrefix": {
            "value": "192.168.0.0/16",
            "metadata": {
                "description": "Enter the addressPrefix of Gateway Subnet"
            }
        },
        "emailAlert": {
            "value": "abc@gmail.com",
            "metadata": {
                "description": "Enter the email id for metric alert"
            }
        },
        "LNGonpremAddressPrefix": {
            "value": ["10.0.0.0/16"],
            "metadata": {
                "description": "Enter the unique addressPrefixes of on-premises network"
            }
        },
        "gatewayPublicIpAddress": {
            "value": "X.X.X.X",
            "metadata": {
                "description": "Enter the local host's public ip address that will be used in Local Network Gateway"
            }
        },
        "sharedKey": {
            "value": "Password@1234",
            "metadata": {
                "description":"Enter the PSK value used for Connections"
            }
        }
    }
}